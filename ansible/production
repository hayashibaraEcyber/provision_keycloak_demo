# 本番環境に合わせて修正

[keycloak]
35.72.112.46

[web]
3.115.161.215

[ldap]
192.168.33.14

[all:vars]
# keycloak
kc_server_domain=demo-kc.cypochi.com
kc_server_email=hayashibara@e-cyber.co.jp

# web
web_server_domain=demo-web.cypochi.com
web_server_email=hayashibara@e-cyber.co.jp

## web用 本番適用の時は書き換える
odic_realm=demouser
odic_client_id=apache24
odic_client_secret=xxx

# ldap
ldap_server_domain=demo-ldap.cypochi.com
ldap_suffix="dc=demo-ldap,dc=cypochi,dc=com"

## ldap用 passwordは本番適用の時は書き換える
ldap_password=xxx